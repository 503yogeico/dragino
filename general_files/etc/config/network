
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option type 'bridge'
	option proto 'static'
	option dns '8.8.8.8'
	option ifname 'eth0'
	option netmask '255.255.255.0'
	option ipaddr '10.130.1.1'
	option gateway '255.255.255.255'

config interface 'wan'
	option ifname 'eth1'
	option proto 'dhcp'
	
config interface 'cellular'
	option proto '3g'
	option device '/dev/ttyUSB2'
	option ipv6 'auto'
	option apn ''
	option dialnumber '*99#'
	option service 'umts'
	option pincode ''
	option username ''
	option password ''
  	option auto '0'
