config settings 'auto_update'
        option update_url '172.31.255.253'
        option update_protocol 'tftp'
        option mac_identify '1'
        option check_internet_host 'www.163.com'
        option current_ver '0.1'

config settings 'update_time'
        option update_on_boot '0'
        option update_on_time '0'
        option hour '23'
        option min '23'
        option date_of_month '*'
        option month '*'
        option date_of_week '*'

config settings 'package_info'
        option opkg_file '/etc/opkg/distfeeds.conf'
        option selected_packages 'pkg_name_1 pkg_name_2'
