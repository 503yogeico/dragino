	option usreg_domain '1'
	option maxassoc '30'
	option channel '6'
	option ap_disable '0'
	option ssid 'dragino-1431e4'
	option encryption 'none'

config secn 'mesh'
	option mesh_enable '0'
	option mpgw 'OFF'

config secn 'dhcp'
	option maxleases '40'
	option dns '8.8.8.8'
	option dns2 '8.8.4.4'
	option enable 'checked'
	option subnet '255.255.255.0'
	option leaseterm '7200'
	option dhcp_auth 'unchecked'
	option startip '10.130.1.200'
	option endip '10.130.1.240'
	option router '10.130.1.1'
	option FallbackIP 'enable'
	option domain 'dragino-net'

config secn 'http'
	option limitip '0'
	option enssl '0'
	option pw_preset '1'
	option auth 'checked'

config secn 'modem'
